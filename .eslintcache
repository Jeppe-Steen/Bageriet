ute>\n\n                    <Route exact path={`${url}/:categoryId`} >\n                        <CategoryProducts />\n                    </Route>\n\n                    <Route exaxt path={`${url}/:categoryId/:productId`} >\n                        <ProductDetails loginData={props.loginData} setLoginData={props.setLoginData} />\n                    </Route>\n                </Switch>\n            </section>\n        </main>\n    )\n} ","/Users/jeppe_steen/bageriet/src/Sites/ContactPage/ContactPage.jsx",[],"/Users/jeppe_steen/bageriet/src/Sites/LoginPage/LoginPage.jsx",[],"/Users/jeppe_steen/bageriet/src/Components/Slider/Slider.jsx",["101"],"import React, { useState, useEffect } from 'react';\n\nimport Style from './Slider.module.scss';\nimport Slide1 from '../../Assets/slide1.jpg';\nimport Slide2 from '../../Assets/slide2.jpg';\nimport Slide3 from '../../Assets/slide3.jpg';\n\n\nexport function Slider() {\n    let imgArr = [\n        {id: 0, image: Slide1}, \n        {id: 1, image: Slide2}, \n        {id: 2, image: Slide3},\n    ];\n\n    const [count, setCount] = useState(0);\n\n    const handleClick = sliderIndex => () => setCount(sliderIndex);\n    \n    function nxt() { let next = (count < imgArr.length - 1) ? setCount(count + 1) : setCount(0); return next; };\n    function pre() { let prev = (count <= 0) ? setCount(imgArr.length - 1) : setCount(count - 1); return prev; };\n\n    let sliderTekst = [\n        'Vi elsker at bage brød',\n        'Kager kan vi også lave',\n        'Vi kan også levere et stort udvalg'\n    ];\n\n   /*useEffect(() => {\n        let timer = setInterval(nxt, 5000);\n    }, [nxt]);*/\n\n    return (\n        <section className={Style.slider}>\n            <img className=\"imageContainer\" src={imgArr[count].image} alt=\"\"/>\n            <h1 className={Style.heading}>{sliderTekst[count]}</h1>\n            <svg className={Style.btn} onClick={pre} xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 63.47 111.04\"><g id=\"Layer_2\" data-nam